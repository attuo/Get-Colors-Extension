{
  "name": "GetColors",
  "version": "1.0",
  "description": "Extension for getting website color codes easily",
  "permissions": ["<all_urls>", "tabs"],
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "32": "icons/favicon-32x-32.png",
    "192": "icons/favicon-192x-192.png"
  },
  "manifest_version": 2
}
